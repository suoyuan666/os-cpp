// -*- C++ -*-

namespace std {
template <typename T>
struct is_nothrow_move_constructible;

template <typename T>
struct is_nothrow_move_assignable;

template <typename T>
constexpr void swap(T& a, T& b) noexcept(
    std::is_nothrow_move_constructible<T>::value &&
    std::is_nothrow_move_assignable<T>::value) {
  T temp = move(a);
  a = move(b);
  b = move(temp);
}

}  // namespace std
