// -*- C++ -*-
#pragma once
#include <cstdint>

namespace std {
auto memset(void* dest, int ch, uint32_t count) -> void*;
auto memmove(void* dest, const void* src, uint32_t count) -> void*;
auto strncpy(char* __restrict__ dest, const char* __restrict__ src,
             uint32_t count) -> char*;
}  // namespace std
